    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "image=cv2.imread('s1200.png')\n",
    "\n",
    "kernel = np.array([-0.5, 0.5])\n",
    "\n",
    "dst = cv2.filter2D(image, -1, kernel)\n",
    "cv2.imwrite(\"filtered.png\", dst)"
